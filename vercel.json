{
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/styles/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/posts/:id(\\d+)-:title*",
      "destination": "/posts/:id",
      "permanent": true
    }
  ]
}
